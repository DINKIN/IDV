  <method request="/entry/show"      method="processEntryShow"  name="Home" toplevel="true" ishome="true"/>

  <method request="/entry/show/*"      method="processEntryShow"/>


  <method request="/entry/searchform"  method="processEntrySearchForm"   name="Search" toplevel="true"/>
  <method request="/entry/search"  method="processEntrySearch"     />
<!--
  <method request="/list/home"   method="processListHome"      name="List" toplevel="true"/>
-->
  <method request="/list/show"   method="processListShow"/>
  <method request="/graph/get"   method="processGraphGet"/>




  <method request="/entry/form"      method="processEntryForm"      actions="edit"/>
  <method request="/entry/change"    method="processEntryChange"    actions="edit"/>
  <method request="/entry/delete"    method="processEntryDelete"    actions="edit"/>

  <method request="/entry/new"       method="processEntryNew"       actions="new"/>

  <method request="/comments/show"   method="processCommentsShow"/>
  <method request="/comments/add"   method="processCommentsAdd"     actions="comment"/>
  <method request="/comments/edit"   method="processCommentsEdit"   actions="edit"/>

  <method request="/entry/delete"    method="processEntryDelete"    actions="edit"/>

