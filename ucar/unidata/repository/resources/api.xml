  <method request="/entry/get/*"     method="processEntryGet"      cancache="false"/>

  <method handler="metadatamanager" request="/metadata/form"   method="processMetadataForm" actions="edit"/>
  <method handler="metadatamanager" request="/metadata/addform"   method="processMetadataAddForm" actions="edit"/>
  <method handler="metadatamanager" request="/metadata/add"   method="processMetadataAdd" actions="edit"/>
  <method handler="metadatamanager" request="/metadata/change"   method="processMetadataChange" actions="edit"/>


  <method request="/association/add"   method="processAssociationAdd"/>
 
  <method request="/getentries"        method="processGetEntries" />

  <method request="/getmap"            method="processMap"       cancache="false"/>
  <method request="/processfile"    method="processFile"        cancache="false"/>
  <method  request="/message"      method="processMessage"    cancache="false" />




  <!-- Note: the admin property below gets applied to all following methods  that do not have the admin attribute -->
  <property name="admin" value="true"/>
  <property name="cancache" value="false"/>



  <property name="handler" value="usermanager"/>
  <method  request="/user/login"    method="processLogin" admin="false"/>
  <method  request="/user/logout"   method="processLogout" admin="false"/>
  <method  request="/user/home"     method="processHome" admin="false"/>
  <method  request="/user/settings" method="processSettings" admin="false"/>
  <method  request="/user/cart"     method="processCart" admin="false"/>

  <method  request="/user/list"     method="adminUserList" />
  <method  request="/user/edit"     method="adminUserEdit" />
  <method  request="/user/new"      method="adminUserNew"/>


  <property name="handler" value="admin"/>
  <method  request="/admin/import/catalog"       method="adminImportCatalog"  />
  <method  request="/admin/sql"          method="adminSql"       />
  <method  request="/admin/startstop"    method="adminDbStartStop" />
  <method  request="/admin/settings"    method="adminSettings"   name="Admin" toplevel="true"/>
  <method  request="/admin/settings/do"    method="adminSettingsDo"/>
  <method  request="/admin/tables"       method="adminDbTables"  />
  <method  request="/admin/actions"       method="adminActions"  />
  <method  request="/admin/stats"        method="adminStats"     />
  <method  request="/admin/harvesters"   method="adminHarvesters"/>
  <method  request="/admin/cleanup"        method="adminCleanup" />
</api>
